<?php
$fechasub = date("Y.m.d");
echo $fechasub;
echo "<br>";
$diasub = date("d");
echo $diasub;
echo "<br>";
$messub = date("m");
echo $messub;
echo "<br>";
$horasub = date("h:i:sa");
echo $horasub;
echo "<br>";
$nomsemsub = date("l");
echo $nomsemsub;
echo "<br>";
?> 

 <?php
$host = gethostbyaddr($_SERVER["REMOTE_ADDR"]);
echo $host;
echo "<br>";

$ip = gethostbyname("www.w3schools.com");
echo $ip;
echo "<br>";

$portnum = getservbyname("http", "tcp");
echo $portnum;
echo "<br>";

?> 

